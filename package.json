{
  "name": "oah-ui",
  "version": "2.0.7",
  "description": "React Styled Components with bootstrap grid system",
  "main": "dist/index.cjs.js",
  "jsnext:main": "dist/index.esm.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/AhmedElywa/oah-ui.git",
  "author": "Ahmed Elywa",
  "license": "MIT",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "rollup -c",
    "pro": "cross-env NODE_ENV=production BABEL_ENV=production rollup -c",
    "start": "rollup -c -w"
  },
  "peerDependencies": {
    "react": ">= 16.8.3",
    "react-dom": ">= 16.8.3",
    "styled-components": ">= 4.1.3"
  },
  "dependencies": {
    "react-select": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@types/node": "^13.7.0",
    "@types/react": "^16.9.33",
    "@types/react-dom": "^16.9.6",
    "@types/react-select": "^3.0.11",
    "@types/styled-components": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "acorn": "^7.1.0",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "gh-pages": "^2.2.0",
    "husky": "^4.2.3",
    "install-peers-cli": "^2.2.0",
    "prepend-file": "^1.3.1",
    "prettier": "^2.0.4",
    "pretty-quick": "^2.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rollup": "^2.3.4",
    "rollup-plugin-typescript2": "^0.27.0",
    "styled-components": "^5.1.0",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --pattern \"**/*.*(js|jsx|tsx|ts)\" --staged"
    }
  },
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/AhmedElywa/oah-ui",
    "email": "ahmed.elywa@icloud.com"
  },
  "homepage": "https://oah-ui.oahtech.io"
